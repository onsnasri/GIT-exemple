pipeline {
    agent any
    tools {
        jdk 'JDK21'
    }
    stages {
        stage('Checkout code')
        {
            steps {
                git branch: 'master', url: 'https://github.com/onsnasri/GIT-exemple.git'
            }
        }
        stage('Compile code')
        {
            steps {
                sh 'javac -d bin src/application/test.java'
            }
        }
        stage('Execute code')
        {
            steps {
                sh 'java -cp bin application.test'
            }
       }
       stage('display message')
       {
            steps {
                echo 'Testing github webhook4'
            }
       }
    }
}
